# Path or URL for Qdrant
QDRANT_URL=http://localhost
QDRANT_PORT=6333

# Default embedding model to use (must match a SentenceTransformer/HuggingFace model name)
DEFAULT_EMBEDDING_MODEL=qdrant/multilingual-e5-large-onnx
QDRANT_EMBEDDING_MODEL=qdrant/multilingual-e5-large-onnx
HF_EMBEDDING_MODEL=intfloat/multilingual-e5-large

# Optional: log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Optional: local model cache settings
# Maximum number of models to cache concurrently
MODEL_CACHE_LIMIT=1
# Directory to store downloaded SentenceTransformer models
LOCAL_MODEL_PATH=../data/models
